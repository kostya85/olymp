use test;

CREATE TABLE `promotions` (
	`id` INT NOT NULL AUTO_INCREMENT,
	`name` VARCHAR(255) NOT NULL,
	`description` VARCHAR(255) NOT NULL,
	`prizes` VARCHAR(255) NOT NULL,
	`participants` VARCHAR(255) NOT NULL,
	PRIMARY KEY (`id`)
);

CREATE TABLE `prizes` (
	`id` INT NOT NULL,
	`description` VARCHAR(255) NOT NULL
);

CREATE TABLE `competitors` (
	`id` INT NOT NULL AUTO_INCREMENT,
	`name` VARCHAR(255) NOT NULL,
	PRIMARY KEY (`id`)
);